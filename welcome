#include <iostream>
using namespace std;

int main(int argc, char* argv[])
{
	if (argc != 2)
	{
		cout << "usage : " << endl;
		cout << "   " << "master" << "   " << "user" << endl;
	}
	else
	{
		cout << "Hello, " << argv[1] << "." << endl;
		cout << "My name is " << argv[0] << endl;
		cout << "Welcome to the CLI's world." << endl;
	}
	return 0;
}
